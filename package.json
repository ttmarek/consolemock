{
  "name": "consolemock",
  "version": "1.1.0",
  "description": "A tool for testing console logs",
  "main": "lib/index.js",
  "typings": "./index.d.ts",
  "scripts": {
    "test": "yarn run lint && jest --watch",
    "lint": "eslint src",
    "prettier": "prettier --single-quote --trailing-comma es5 --write \"{src,__{tests,mocks}__}/**/*.js\"",
    "check": "yarn run lint && jest --coverage",
    "build": "babel src --out-dir lib --ignore __tests__",
    "prepublishOnly": "yarn run build"
  },
  "files": [
    "index.d.ts",
    "lib",
    "src"
  ],
  "keywords": [
    "mock console",
    "test console",
    "console mock",
    "console",
    "test"
  ],
  "author": "Thomas Marek",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ttmarek/consolemock.git"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.6.0",
    "eslint": "^4.9.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-prettier": "^2.3.1",
    "jest": "^21.2.1",
    "prettier": "1.7.4"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  },
  "dependencies": {
    "stringify-object": "^3.1.0"
  }
}
